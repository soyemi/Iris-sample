{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "defaultSeverity": "off",
  "rules":{
    "interface-name":false,
    "no-string-literal":false,
    "ordered-imports":false
  }
}
